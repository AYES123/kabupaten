- @page_title = "Kabupatens"
- @body_class = "kabupatens"

%table.table.table-striped
  %tr
    %th Province
    %th Name
    %th Area
    %th Population
    %th
    %th

  - @kabupatens.each do |kabupaten|
    %tr
      %td= kabupaten.province_id
      %td= link_to kabupaten.name, kabupaten
      %td= kabupaten.area
      %td= kabupaten.population
      %td.btn.btn-mini= link_to 'Edit', edit_kabupaten_path(kabupaten)
      %td.btn.btn-mini.btn-danger= link_to 'Destroy', kabupaten, :confirm => 'Are you sure?', :method => :delete

%br

.btn.btn-primary= link_to 'New', new_kabupaten_path
